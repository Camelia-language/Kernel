# Logical operators

print(true && false)
# => false

print(true && true)
# => true

print("hi" && "there")
# => there

print(true || false)
# => true

print(false || 1)
# => 1

print(false || 3 && nil)
# => null

print(!true)
# => false

print(!"hi")
# => false

print(!false)
# => true

print(!!1)
# => true

print(1 < 4)
# => true

print(5 < 4)
# => false